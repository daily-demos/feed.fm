{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "plugins": ["prettier"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "prettier/prettier": ["error", { "singleQuote": true }],
        "no-use-before-define": ["error", {"functions": false}],
        "no-unused-vars": [
          "error",
          { "argsIgnorePattern": "^_" }
        ]
    }
}
